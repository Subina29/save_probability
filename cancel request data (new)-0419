dfTotal=pdq.read_gbq(sql, project_id=project_id, dialect='standard')
dfTotal.drop('mrr_amount',axis=1,inplace=True)
dfTotal=dfTotal.drop(dfTotal[dfTotal['created_date']>datetime.datetime(2019,2,1)].index)

